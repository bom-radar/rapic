set(PACKAGE_VERSION "@GPATS_VERSION@")

# enforce major version match
if ("${PACKAGE_FIND_VERSION}" VERSION_GREATER @GPATS_VERSION@ OR NOT "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @GPATS_VERSION_MAJOR@)
  set(PACKAGE_VERSION_COMPATIBLE FALSE)
  set(PACKAGE_VERSION_EXACT FALSE)
else()
  set(PACKAGE_VERSION_COMPATIBLE TRUE)
  if ("${PACKAGE_FIND_VERSION}" VERSION_EQUAL @GPATS_VERSION@)
    set(PACKAGE_VERSION_EXACT TRUE)
  endif()
endif()

# Config file for the rapic package
# Defines the following variables:
#  RAPIC_INCLUDE_DIRS - include directories for rapic
#  RAPIC_LIBRARIES    - libraries to link against

get_filename_component(RAPIC_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include("${RAPIC_CMAKE_DIR}/rapic-targets.cmake")
set(RAPIC_INCLUDE_DIRS "${RAPIC_CMAKE_DIR}/@REL_INCLUDE_DIR@")
set(RAPIC_LIBRARIES rapic)
